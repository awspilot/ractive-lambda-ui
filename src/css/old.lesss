.ractive-lambda-ui {





	/* bootstrap */
	a {
		color: @text-color;
	}






	.btn-group>.btn {
		position: relative;
		float: left;
	}
	.btn-group, .btn-group-vertical {
		position: relative;
		display: inline-block;
		vertical-align: middle;
	}
	.btn-group>.btn:first-child {
		margin-left: 0;
	}
	.btn-group>.btn:first-child:not(:last-child):not(.dropdown-toggle) {
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
	}

	.btn-group-xs>.btn,
	.btn-xs {
		padding: 1px 5px;
		font-size: 12px;
		line-height: 1.5;
	}
	.btn-group>.btn:last-child:not(:first-child), .btn-group>.dropdown-toggle:not(:first-child) {
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
	}
	.btn-group>.btn+.dropdown-toggle {
		padding-right: 8px;
		padding-left: 8px;
	}
	.btn-group .btn+.btn, .btn-group .btn+.btn-group, .btn-group .btn-group+.btn, .btn-group .btn-group+.btn-group {
		margin-left: -1px;
	}

	.btn-default.active.focus, .btn-default.active:focus, .btn-default.active:hover, .btn-default:active.focus, .btn-default:active:focus, .btn-default:active:hover, .open>.dropdown-toggle.btn-default.focus, .open>.dropdown-toggle.btn-default:focus, .open>.dropdown-toggle.btn-default:hover {
		color: #333;
		background-color: #d4d4d4;
		border-color: #8c8c8c;
	}
	.btn.active.focus, .btn.active:focus, .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus {
		outline: 5px auto -webkit-focus-ring-color;
		outline-offset: -2px;
	}

	.btn-group-sm>.btn,
	.btn-sm {
		padding: 5px 10px;
		font-size: 12px;
		line-height: 1.5;

	}


	.dropdown-menu {
		position: absolute;
		top: 100%;
		left: 0;
		z-index: 1000;
		display: none;
		float: left;
		min-width: 160px;
		padding: 5px 0;
		margin: 2px 0 0;
		font-size: 14px;
		text-align: left;
		list-style: none;

		-webkit-background-clip: padding-box;
		background-clip: padding-box;
		border-radius: 4px;
		box-shadow: 0 6px 12px rgba(0,0,0,.175);
	}
	.dropdown-menu {
		background-color: @base-background-color;
		border: 1px solid @base-border-color;
	}
	.dropdown-menu.pull-right {
		right: 0;
		left: auto;
	}
	.show {
		display: block!important;
	}





















	.hsplit { color: #999999; }



	miniheader .icon:hover {box-shadow: 0px 0px 1px rgba(255,255,255,.3);}





	content tabhead {position: absolute;top: 0px;left: 0px;right:0px;height: 29px;overflow: hidden;font-size: 0px;z-index:2;}
	content tabhead tab {display: inline-block;height: 28px;line-height: 28px;padding: 0px 10px;cursor: pointer;font-size: 14px;}
	content tabhead tab.active {height: 29px;}
	content tabhead tab .icon {text-align: center;margin-top: 2px;cursor: pointer;font-size: 14px;}
	content tabhead tab .icon:hover {box-shadow: 0px 0px 1px rgba(255,255,255,.3);}
	content tabcontent {position: absolute;top: 28px;left: 0px;right: 0px;bottom: 0px;}






	content tabcontent .tableview { visibility: hidden }
	content tabcontent .tableview.active { visibility: visible }

	content tabcontent .tableview .tablebrowse {display: block;position: absolute;top: 0px;left:0px;right: 0px;bottom: 0px;}

	content tabcontent .tablesqlquery {display: block;position: absolute;top: 0;left:0px;right: 0px;height: 119px;overflow: hidden;}
	content tabcontent .tablequery {display: block;position: absolute;top: 0;left:0px;right: 0px;height: 119px;overflow: hidden; padding: 10px;}
	content tabcontent .tabledatacontrols {position: absolute;top: 120px;left: 0px;right: 0px;height: 28px;padding: 3px 0px 0px 30px;}



	content tabcontent .tabledata {display: block;position: absolute;left:0px;right: 0px;bottom: 0px;overflow: hidden;}




	content tabcontent .tabledata .tabledatahead,
	content tabcontent .tabledata .tabledatarow { display: flex;width: 100%;justify-content: stretch}










	content tabcontent .tabledata .tabledatahead {position: absolute;top: 0px;left: 0px;right: 0px;height: 20px;}


	content tabcontent .tabledata .tabledatarow .zmdi.selectrow {font-size: 14px;}

	content tabcontent .tabledata .tabledatahead > div,
	content tabcontent .tabledata .tabledatarow .tabledatacell {display: flex;height: 20px;align-items: center;overflow: hidden;white-space: nowrap;padding-left: 6px;position: relative;}




	content tabcontent .tabledata .tabledatarow .tabledatacell:before {content: ' ';position:absolute;top: 0px;left:  0px;bottom: 0px;width: 5px;}
	content tabcontent .tabledata .tabledatarow .tabledatacell:after  {content: ' ';position:absolute;top: 0px;right: 0px;bottom: 0px;width: 5px;}



	content tabcontent .tabledata .tabledatacontent {position: absolute;top: 23px;left: 0px;right: 0px;bottom: 0px;overflow-x: auto;}

	content tabcontent .tabledata .tabledatahead > div {font-size: 12px;}
	content tabcontent .tabledata .tabledatarow .tabledatacell {font-size: 11px;}




	.scrollarea:hover > span {position: absolute; right: 0px;width: 3px;background-color: rgba(255, 255, 255, 0.3);z-index: 1;}

	.pull-right {float: right;}

	content {position:absolute;display: block;top: 0px;left: 265px;right: 0px;bottom: 0px;}

}







.ractive-lambda-ui {
	header {background-color: #232f3e;/*box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);*/ }








	content tabhead tab:first-child {}








	content tabcontent .tabledata {border-top: 1px solid transparent;}





	content tabcontent .tabledata .tabledatarow .tabledatacell.t-K {color: #8ea7aa;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-HASH {color:#004b91;cursor: pointer;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-HASH:hover a {text-decoration: underline;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-S {color: #a79b9b;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-N {color: green;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-BOOL {color: #ff7676;}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-NULL {color: grey}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-SS {}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-NS {}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-L {}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-M {}
	content tabcontent .tabledata .tabledatarow .tabledatacell.t-U {background-size: 13px 13px;background-image:linear-gradient(90deg, fade(#000, 20% ) 50%, transparent 50%),linear-gradient(       fade(#000, 20% ) 50%, transparent 50%);}


	content tabcontent .tabledata .tabledatarow:hover .tabledatacell.t-HASH {color:@text-color-highlight;}

	content tabcontent .tabledata .tabledatarow.selected .tabledatacell {color: @text-color-selected;}

	content tabcontent .tabledata .tabledatarow.selected:hover .tabledatacell {color: #fff;}
}


content tabcontent {position: absolute;top: 28px;left: 0px;right: 0px;bottom: 0px;}


.function-details-head > tabhead {position: absolute;top: 0px;left: 0px;right:0px;height: 38px;overflow: hidden;font-size: 0px;}
.function-details-head > tabhead tab {display: inline-block;line-height: 28px;padding: 0px 25px 0px 25px; margin-bottom: 10px; cursor: pointer;font-size: 14px;}
.function-details-head > tabhead tab.active {}


.function-details-head > tabhead { padding-bottom: 10px;border-bottom: 1px solid #cccccc;}
.function-details-head > tabhead tab:first-child {border-left: 0px;}
.function-details-head > tabhead tab {border-left: 1px solid #cccccc;font-weight: bold;color: #545b64;}
.function-details-head > tabhead tab.active {color: #eb5f07;position: relative;}
.function-details-head > tabhead tab.active:after {content: '';position: absolute;left: 0px;right: 0px;bottom: -9px;border-bottom: 2px solid #000;}




.editor-menubar {position: absolute;top: 0px;left: 0px;right: 0px;height: 40px;box-sizing: border-box;border-bottom: 1px solid #eaeaea;background-color: #f2f3f3;}



	/* editor tree */
	ul { list-style-type: none; padding: 5px;font-size: 12px;}
	ul > li {user-select: none;}
	ul > li > a { text-decoration: none; color: #333; cursor: pointer; }
	ul > li > svg {cursor: pointer;}
